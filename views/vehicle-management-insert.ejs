      <section class="ftco-section">
    	<div class="container">
    <h2>車両登録フォーム</h2>
    <% for(var value of values) { %>
      
      <% } %>
      <% var newvalue = value.carid+1 %>
      
    <form action='/vehicleregist?id=<%= newvalue  %>'  method="POST" enctype="multipart/form-data">

        <label>車両メーカー</label>
            <select name="carbrand" >
                <option value="HONDA">HONDA</option>
                <option value="TOYOTA">TOYOTA</option>
                <option value="MAZDA">MAZDA</option>
                <option value="LEXUS">LEXUS</option>
                <option value="BMW">BMW</option>
                <option value="AUDI">AUDI</option>
                <option value="MERCEDESZ-BENZ">MERCEDESZ-BENZ</option>
              </select>

            <br>
        <label>車種名</label>
            <input type="text" name="carname">
            <br>
        <label>年式</label>
            <input type="text" name="caryear">
            <br>
        <label>色</label>
            <input type="text" name="carcolor">
            <br>
        <label>販売者ID</label>
            <input type="text" name="sellerid">
            <br>
        <label name='price'> 
          <p> 最終価格： <input type='text' name='price' placeholder='12000'> </p>
          
          <p><span>画像登録 :</span> <br><br><input type='file' name='mypic' /><br><br>
             <input type='file' name='mypic' /><br><br><input type='file' name='mypic' /></p>

        <button>登録</button>
    </form>

